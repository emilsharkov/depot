{
  "name": "depot-monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build -w depot",
    "dev": "npm run dev -w depot",
    "test": "npm run test -w depot",
    "test:run": "npm run test:run -w depot",
    "typecheck": "npm run typecheck -w depot",
    "example:dev": "npm run build -w depot && npm run dev -w example",
    "example:build": "npm run build -w example",
    "example:preview": "npm run preview -w example"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  }
}
